{% if title %}
  <h3>{{ title }}</h3>
{% endif %}
{% for row in rows %}
  {%
    set row_classes = [
      default_row_class ? 'views-row',
    ]
  %}
  {% if default_row_class %}
    <div{{ row.attributes.addClass(row_classes,'col-12 col-md-4') }}>
      {{- row.content -}}
    </div>
  {% else %}
    {{- row.content -}}
  {% endif %}
{% endfor %}
